{
  "npmScope": "yeti",
  // FIXME: Till Nx 10
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/src/tasks-runner/tasks-runner-v2",
      "options": {
        "cacheableOperations": ["build", "test", "lint"]
      }
    }
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "projects": {
    "webapp": {
      "tags": ["app-module"]
    },
    "api": {
      "tags": ["api-module"]
    },
    "gen": {
      "tags": ["shared-module"]
    },
    "home": {
      "tags": ["layout", "entry-module"]
    },
    "not-found": {
      "tags": ["entry-module"]
    },
    "core": {
      "tags": ["core-module"]
    },
    "dashboard": {
      "tags": ["layout", "entry-module"]
    },
    "admin": {
      "tags": ["layout", "entry-module"]
    },
    "shared": {
      "tags": ["shared-module"]
    },
    "blog": {
      "tags": ["private-module"]
    },
    "accounts": {
      "tags": ["child-module"]
    }
  }
}
